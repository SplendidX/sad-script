function print-list (lst) {
   for x in lst {
      (println x)
   }
}

var a = (list 1 2 3 4)
(print-list a)

(println "-")
(print-list (pipe a (where \x [[x % 2] = 0]) reverse))
(print-list (pipe a (where \x [[x % 2] = 1]) reverse (skip 1) reverse))

(println "-")
(print-list [1 ... 4])

(println "-")
var b = [1 ... 100]
(print-list (pipe b (skip 7) (take 2)))